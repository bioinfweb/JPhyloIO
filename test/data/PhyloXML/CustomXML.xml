<phyloxml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.phyloxml.org" 
	xsi:schemaLocation="http://www.phyloxml.org http://www.phyloxml.org/1.10/phyloxml.xsd" xmlns:ex="http://example.org">
    <phylogeny rooted="true">
        <name>Tree 1</name>
        <clade>	
			<name>1</name>
            <clade>
                <name>A</name>
				<ex:exampleTag>
					<ex:subTag ex:attr="A">example Text 1</ex:subTag>
				</ex:exampleTag>
				<ex:secondExampleTag/>
            </clade>
            <clade>
				<name>2</name>
				<taxonomy>
					<id>ID1</id>
					<ex:exampleTag>
						<ex:subTag ex:attr="B">example Text 2</ex:subTag>
					</ex:exampleTag>
				</taxonomy>
                <clade>                    
					<name>B</name>
					<sequence>
						<name>ID1</name>
						<ex:exampleTag>
							<ex:subTag ex:attr="C">example Text 3</ex:subTag>
						</ex:exampleTag>
					</sequence>
                </clade>
                <clade>
					<name>C</name>
					<events>
						<duplications>50</duplications>						
						<ex:illegalTag>70</ex:illegalTag>
					</events>
                </clade>
            </clade>
        </clade>
		<ex:exampleTag>
			<ex:subTag ex:attr="D">example Text 4</ex:subTag>
		</ex:exampleTag>
    </phylogeny>
	<ex:exampleTag>
		<ex:subTag ex:attr="E">example Text 5</ex:subTag>
	</ex:exampleTag>
</phyloxml>