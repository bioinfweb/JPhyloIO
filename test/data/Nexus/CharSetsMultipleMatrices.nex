#NEXUS

BEGIN TAXA;
	DIMENSIONS NTAX = 3;
	TAXLABELS 
		A
		B
		C; 
END; 

BEGIN Characters;
  Title matrix1;
  Dimensions NTax=3 NChar=7;
  Format DataType = DNA Missing=? Gap= -;
  Matrix A CGGTCAT
         B CG-TCTT 
         C CG-TC-T;
END;

BEGIN Characters;
  Title matrix2;
  Dimensions NTax=3 NChar=7;
  Format DataType = DNA Missing=? Gap= -;
  Matrix A AGGT-AT
         B AC-GCTC 
         C AG-TC-T;
END;

BEGIN Sets;
  TITLE sets1;
  LINK CHARACTERS=matrix1;
  CharSet set01 = 2-4 6;
END;

BEGIN Sets;
  TITLE sets2;
  LINK CHARACTERS=matrix2;
  CharSet set01 = 1-3;
END;

BEGIN Sets;
  TITLE sets3;
  CharSet set01 = 1-2;
END;